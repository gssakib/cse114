abstract class Cards{
	private String name = "";
	private int val;
	
	public Cards(String name, int val){
		this.name = name;
		this.val = val;
	}
	
	
	public void setName(String name){
		name = this.name;
	}
	
	public String getName(){
		return this.name;
	}
	
	public void setVal(int val){
		val = this.val;
	}
	
	public int getVal(){
		return this.val;
	}
		
}



class Dagger extends Cards{
	public Dagger(){
		super("Dagger", 1);
		
	}
}

class Sword extends Cards{
	public Sword(){
		super("Sword", 2);
		
	}
}

class MorningStar extends Cards{
	public MorningStar(){
		super("Morning", 3);
		
	}
}

class WarAxe extends Cards{
	public WarAxe(){
		super("WarAxe", 4);
		
	}
}

class Halberd extends Cards{
	public Halberd(){
		super("Halberd", 5);
		
	}
}

class LongSword extends Cards{
	public LongSword(){
		super("LongSword", 6);
		
	}
}

class Archer extends Cards{
	public Archer(){
		super("Archer", 7);
		
	}
}

class Sheild extends Cards{
	public Sheild(){
		super("Sheild", 0);
		
	}
}

class Crown extends Cards{
	public Crown(){
		super("Crown", -1);
		
	}
}