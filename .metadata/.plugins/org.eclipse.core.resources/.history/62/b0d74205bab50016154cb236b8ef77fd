import java.util.ArrayList;

public class Homework8Methods {
	// part 1
		public static int findTimes10(int[] nums) {
			int lower = 0;
			
			return findTimes10Helper(nums, lower);
		}

		
		
		public static int findTimes10Helper(int[] nums, int lower){
			
			if(lower+1 == nums.length-1){
				return -1;
			}
			if(nums[lower] != 10*nums[lower + 1] ){
				System.out.println(lower);
				findTimes10Helper(nums, lower + 1);
			}else if(nums[lower] == 10*nums[lower + 1] ){
				return (lower);
			}else{
				return -1;
			}
			return 1;
		}
		
		
		
