
public class DessertCafe {

	public static void main(String[] args) {
		
	}

}


class Dessert{
	private String name;
	private int cost;
	public Dessert(String name, int cost){
		this.name = name;
		this.cost = cost;
	}
	
	
	public String getName(){
		return this.name;
		
	}
	
	public int getCost(){
		
		return this.cost;
		
	}
	
}	
	
class Cake extends Dessert{
	
	private int layer;
	public Cake(String name, int layer){
		super(name, 15);
		this.layer = layer;
		
	}
	
	
		
}
	
	
	
	
	
	
	
}
