import java.util.Scanner;

public class PalindromicDate {

	public static void main(String[] args) {
		Scanner stdin = new Scanner(System.in);
		
		System.out.print("Enter month: ");
		String month = stdin.nextLine();
		int monthNo = 0;
		
		System.out.print("Enter day: ");
		int day = stdin.nextInt();
		
		System.out.print("Enter year: ");
		int year = stdin.nextInt();
		
		
		if((month.substring(0,2)).equals("ja")){
			monthNo = 1;
			
		}else if((month.substring(0,1)).equals("f")){
			monthNo = 2;
		}else if((month.substring(0,3)).equals("mar")){
			monthNo = 3;
		}else if((month.substring(0,2)).equals("ap")){
			monthNo = 4;
		}else if((month.substring(0,3)).equals("may")){
			monthNo = 5;
		}else if((month.substring(0,3)).equals("jun")){
			monthNo = 6;
		}else if((month.substring(0,3)).equals("jul")){
			monthNo = 7;
		}else if((month.substring(0,2)).equals("au")){
			monthNo = 8;
		}else if((month.substring(0,1)).equals("s")){
			monthNo = 9;
		}else if((month.substring(0,1)).equals("o")){
			monthNo = 10;
		}else if((month.substring(0,1)).equals("n")){
			monthNo = 11;
		}else {
			monthNo = 12;
		}
		System.out.println(monthNo);
		

	}

}
